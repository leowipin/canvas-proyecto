<div id="<%= dom_id course %>">

  <p>
    <strong>Course id:</strong>
    <%= course.id %>
  </p>

  <p>
    <strong>Course name:</strong>
    <%= course.course_name %>
  </p>

  <p>
    <strong>Teacher:</strong>
    <%= course.teacher %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= course.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= course.end_time %>
  </p>

</div>
